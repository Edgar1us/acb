<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="modelo.Entrenador" table="entrenador">
      
      <id name="idEntrenador" column="idEntrenador" type="integer">
          <generator class="increment"></generator>
      </id>
      
      <property name="nombreEntrenador" column="nombreEntrenador" type="string"/>
      <property name="edad" column="edad" type="integer"/>
      <property name="titulo" column="titulo" type="boolean"/>
      
      
  </class>   
  
  
  
</hibernate-mapping>
